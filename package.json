{
  "name": "rnjs",
  "version": "0.1.0",
  "description": "A small reactive data and utility library",
  "main": "dist/rn.cjs.js",
  "jsnext:main": "dist/rn.js",
  "config": "",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:es6": "rollup -c -o dist/rn.js",
    "postbuild:es6": "tools/strip.sh dist/rn.js",
    "build:cjs": "rollup -c -f cjs -o dist/rn.cjs.js",
    "postbuild:cjs": "tools/strip.sh dist/rn.cjs.js",
    "build:iife": "rollup -c -f iife -o dist/rn.iife.js",
    "postbuild:iife": "tools/strip.sh dist/rn.iife.js",
    "prebuild": "npm run clean -s",
    "build": "npm run build:es6 && npm run build:cjs && npm run build:iife",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Raymond Neilson",
  "license": "ISC",
  "devDependencies": {
    "rollup": "^0.26.3",
    "rollup-plugin-json": "^2.0.0"
  }
}
