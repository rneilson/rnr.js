{
  "name": "rnjs",
  "version": "0.1.0",
  "description": "A small reactive data and utility library",
  "main": "dist/rn.cjs.js",
  "jsnext:main": "dist/rn.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "strip:es6": "grep -v '^\\s*//[^#].*$' dist/rn.js | grep -v '^\\s*$' > dist/rn.min.js",
    "build:es6": "rollup -c",
    "build:cjs": "rollup -c -f cjs -o dist/rn.cjs.js",
    "prebuild": "npm run clean -s",
    "build": "npm run build:es6 && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Raymond Neilson",
  "license": "ISC",
  "devDependencies": {
    "rollup": "^0.26.3",
    "rollup-plugin-json": "^2.0.0"
  }
}
